<launch>
    <!-- include the official gazebo file -->
    <arg name="thrust_config" default="T"/>
    <arg name="gui" default="true"/>
    <arg name="rviz" default="$(eval not arg('gui') )"/>
    
    <include file="$(find vrx_bringup)/launch/sensored_boat.launch">
        <arg name="world_name" value="wayfinding"/>
        <arg name="gui" value="$(arg gui)"/>
    </include>

    <!--<include file="$(find vrx_tasks)/launch/missioncontrol.launch"/>-->

    <include file="$(find vrx_bringup)/launch/rviz.launch" if="$(arg rviz)"/>

</launch>
