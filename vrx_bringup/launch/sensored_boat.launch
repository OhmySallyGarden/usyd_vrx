<?xml version="1.0"?>
<launch>

  <arg name="ground_truth_enabled" default="true"/>
    <include file="$(find vrx_gazebo)/launch/sandisland.launch">
        <arg name="urdf" value="$(find vrx_description)/configs/usyd_wamv.urdf"/>
    </include>
    <group ns = "wamv">
      <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher">
          <param name="publish_frequency" value="30"/>
          <param name="tf_prefix" value="wamv"/>
      </node>

      <node name="odom_to_tf" pkg="vrx_navigation" type="odom_to_tf.py" output="screen">
        <remap from="odom" to="sensors/position/p3d_wamv"/>
        <param name="tf_prefix" value="wamv"/>
      </node>
    </group>

    <include file="$(find vrx_bringup)/launch/rviz.launch"/>

</launch>
